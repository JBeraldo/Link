{
    "name": "jberaldo/link",
    "description": "Microframework inspired by Slimphp",
    "type": "project",
    "require": {
        "php": "^8.1",
        "nikic/fast-route": "^1.3",
        "ext-swoole": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Link\\": "src/",
            "Link\\App\\": "src/app/",
            "Link\\Contract\\": "src/contract/",
            "Link\\Router\\": "src/router/",
            "Link\\Server\\": "src/server/",
            "Link\\Support\\": "src/support/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "swoole/ide-helper": "~5.0.0"
    }
}
